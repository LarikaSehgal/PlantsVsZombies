<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="anch" prefHeight="300" prefWidth="500" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginPageController">
   <children>
      <ImageView fitHeight="311.0" fitWidth="500.0" layoutY="-1.0">
         <image>
            <Image url="@Loading%20page.jpg" />
         </image>
      </ImageView>
      <Button fx:id="ExistingUser" layoutX="182.0" layoutY="177.0" mnemonicParsing="false" onAction="#findUser" prefHeight="27.0" prefWidth="142.0" text="EXISTING USER" />
      <Button fx:id="NewUser" layoutX="183.0" layoutY="140.0" mnemonicParsing="false" onAction="#openNewUser" prefHeight="27.0" prefWidth="142.0" text="NEW USER" />
      <ImageView fitHeight="58.0" fitWidth="102.0" layoutX="392.0" layoutY="251.0">
         <image>
            <Image url="@exit.png" />
         </image>
      </ImageView>
      <Button blendMode="DARKEN" layoutX="411.0" layoutY="257.0" mnemonicParsing="false" onAction="#exit" prefHeight="45.0" prefWidth="63.0" />
   </children>
</AnchorPane>
